{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "bref/bref": ">=2.4.15",
        "bref/symfony-bridge": ">=1.1",
        "cloudevents/sdk-php": "^1.2",
        "doctrine/dbal": "^4.4.0",
        "doctrine/doctrine-migrations-bundle": ">=3.7.0",
        "dragonmantank/cron-expression": "^3.6",
        "nelmio/cors-bundle": "^2.6",
        "opis/json-schema": "^2.6.0",
        "symfony/console": "7.4.*",
        "symfony/doctrine-messenger": "7.4.*",
        "symfony/dotenv": "7.3.*",
        "symfony/expression-language": "7.4.*",
        "symfony/flex": "^2.10.0",
        "symfony/framework-bundle": "7.4.*",
        "symfony/messenger": "7.4.*",
        "symfony/monolog-bundle": "^3.11",
        "symfony/runtime": "7.4.*",
        "symfony/scheduler": "7.4.*",
        "symfony/security-bundle": "7.4.*",
        "symfony/uid": "7.4.*",
        "symfony/yaml": "7.4.*"
    },
    "require-dev": {
        "deptrac/deptrac": "^4.2.1",
        "friendsofphp/php-cs-fixer": "^3.91.0",
        "infection/infection": "^0.30.3",
        "j13k/yaml-lint": "^1.1.7",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.35.1",
        "phpstan/phpstan": "^2.1.32",
        "phpstan/phpstan-strict-rules": "^2.0.7",
        "phpstan/phpstan-symfony": "^2.0.9",
        "phpunit/phpunit": "^12.4.4",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "7.4.*",
        "symfony/phpunit-bridge": "^7.4.0"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        }
    }
}
